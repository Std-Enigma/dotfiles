{
  "base_keymap": "VSCode",
  "vim_mode": true,
  "ui_font_size": 14,
  "buffer_font_size": 13,
  "relative_line_numbers": true,
  "tabs": {
    "file_icons": true,
    "git_status": true
  },
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  },
  "theme": {
    "mode": "system",
    "light": "Catppuccin Latte (lavender)",
    "dark": "Catppuccin Mocha (lavender)"
  }
}
